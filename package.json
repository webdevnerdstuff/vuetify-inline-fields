		"vite-plugin-css-injected-by-js": "^3.3.0",
		"vite-plugin-dts": "^3.6.4",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-static-copy": "^0.17.1",
		"vite-plugin-stylelint": "^5.2.1",
		"vite-plugin-vuetify": "^1.0.2",
